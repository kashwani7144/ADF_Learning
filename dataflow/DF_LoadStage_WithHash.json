{
	"name": "DF_LoadStage_WithHash",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_VIVID_RAW_TABLES",
						"type": "DatasetReference"
					},
					"name": "VivivdRawTable"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"parameters{",
				"     df_sqlQuery as string",
				"}",
				"source(output(",
				"          INDEX_NUM as integer,",
				"          CUSTOMER_ID as string,",
				"          FIRST_NAME as string,",
				"          LAST_NAME as string,",
				"          COMPANY as string,",
				"          CITY as string,",
				"          COUNTRY as string,",
				"          PHONE_1 as string,",
				"          PHONE_2 as string,",
				"          EMAIL as string,",
				"          SUBSCRIPTION_DATE as string,",
				"          WEBSITE as string,",
				"          SRC_ROW_INSRT_TSTMP as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: (toString($df_sqlQuery)),",
				"     format: 'query') ~> VivivdRawTable"
			]
		}
	}
}